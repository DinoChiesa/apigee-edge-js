    "qs": "6.4.0",
    "readline-sync": "1.4.9",
    "request": "2.88.0",
    "sprintf-js": "1.1.2",
    "url-join": "2.0.x",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "4.1.0",
