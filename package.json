{
  "name": "apigee-edge-js",
  "version": "0.4.34",
  "description": "nodejs library for the administration API for Apigee (Edge and X and hybrid).",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter mocha-multi-reporters --reporter-options configFile=multi-reporter-config.json",
    "test1": "mocha --grep '^Proxy.*' --reporter mocha-multi-reporters --reporter-options configFile=multi-reporter-config.json",
    "simpletest": "mocha  --reporter spec",
    "badgegen": "mocha  --reporter @dchiesa/mocha-badge-generator"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DinoChiesa/apigee-edge-js.git"
  },
  "keywords": [
    "apigee",
    "apigee-edge",
    "apigee-x",
    "apigee-hybrid"
  ],
  "author": {
    "name": "Dino Chiesa",
    "email": "dchiesa@google.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "adm-zip": "0.4.11",
    "archiver": "5.3.1",
    "dateformat": "3.0.3",
    "debug": "^4.3.3",
    "netrc": "0.1.3",
    "qs": "6.4.0",
    "readline-sync": "1.4.9",
    "request": "2.88.0",
    "sprintf-js": "1.1.2",
    "url-join": "2.0.x",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "faker": "4.1.0",
    "mocha": "^10.0.0",
    "@dchiesa/mocha-badge-generator": "^0.9.0",
    "mocha-multi-reporters": "^1.1.7"
  },
  "engines": {
    "node": ">= 12.22.0",
    "npm": ">= 8.3.0"
  },
  "overrides": {
    "strip-ansi" : "5.2.0",
    "package-json": "^8.1.0",
    "badge-up" : {
      "svgo" : {
        "css-select" : {
          "nth-check" : "2.0.1"
        }
      }
    }
  }
}
